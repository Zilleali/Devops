.section-title {
    display: block;
    position: relative;
    margin-bottom: 75px;
    color: #222222;
    font-size: 18px;
    font-style: normal;
    font-weight: normal;
    letter-spacing: 0.025em;
    line-height: 1.22;
    text-align: center;
    text-align: center;
    text-transform: uppercase;
}

.section-title > * {
    display: block;
}


.section-title > span {
    padding-bottom: 15px;
}

.section-title h1,
.section-title h2,
.section-title h3,
.section-title h4,
.section-title h5,
.section-title h6 {
    margin: 0;
    color: #222222;
    font-size: 90px;
    font-style: normal;
    font-weight: 600;
    line-height: 1.1;
    text-transform: capitalize;
}

